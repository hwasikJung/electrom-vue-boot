{
  "name": "elec-vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "electron:dev": "concurrently \"npm run backend:dev\" \"vite --mode development\"",
    "electron:build": "vite build && electron-builder",
    "backend:dev": "cd ../backend && .\\gradlew.bat bootRun",
    "gradle:run": "cross-env NODE_ENV=development gradlew.bat bootRun",
    "gradle:bootrun": "gradlew.bat bootRun",
    "fullstack:dev": "concurrently \"npm run backend:dev\" \"vite --mode development\" \"electron .\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@quasar/extras": "^1.17.0",
    "@quasar/vite-plugin": "^1.9.0",
    "@tanstack/vue-query": "^5.80.7",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "echarts": "^5.6.0",
    "pg": "^8.16.0",
    "pg-hstore": "^2.3.4",
    "pinia": "^3.0.3",
    "quasar": "^2.18.1",
    "sass-embedded": "^1.89.2",
    "sequelize": "^6.37.7",
    "vue": "^3.5.13",
    "vue-echarts": "^7.0.3",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^36.3.1",
    "electron-builder": "^26.0.12",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-vue": "~10.0.0",
    "globals": "^16.0.0",
    "prettier": "3.5.3",
    "vite": "^6.2.4",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vite-plugin-vue-devtools": "^7.7.2"
  },
  "main": "dist-electron/index.js",
  "build": {
    "appId": "com.yourapp.id",
    "productName": "DataNet Potal",
    "directories": {
      "output": "dist_electron"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ]
  }
}
